[flake8]
inline-quotes = single
max-line-length = 120
max-complexity = 9
show-source = True
statistics = True
ignore =
    D200
    A002
    W503
per-file-ignores=
    threads/__init__.py:
        D104
        F401
    threads/*/__init__.py:
        D104
        F401
class_attributes_order =
    field,
    meta_class,
    nested_class,
    magic_method,
    property_method,
    static_method,
    class_method,
    method,
    private_method
